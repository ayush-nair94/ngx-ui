  "description": "A small collection of angular 4+ components using semantic-ui css",
  "main": "dist/index.js",
  "scripts": {
    "clean": "rimraf dist/",
    "test": "npm run build && rm -rf ./tests/src/dist && cp -r ./dist ./tests/src/dist && cd tests && ng test --single-run",
    "build:aot": "ngc -p tsconfig.json",
    "build:jit": "tsc -p tsconfig.json",
    "build": "npm run clean && npm run build:aot"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nodeableio/ngx-ui.git"
  },
  "keywords": [
    "angular",
    "breadcrumb"
  ],
  "author": {
    "name": "Julian Lyndon-Smith",
    "email": "julian+ngxui@nodeable.io"
  },
  "types": "dist/index.d.ts",
  "private": false,
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/nodeableio/ngx-ui/issues"
  },
  "homepage": "https://github.com/nodeableio/ngx-ui#readme",
  "peerDependencies": {
    "@angular/common": "^4.0.0",
    "@angular/compiler": "^4.0.0",
    "@angular/core": "^4.0.0",
    "@angular/http": "^4.0.0",
    "@angular/platform-browser": "^4.0.0",
    "@angular/router": "^4.0.0",
    "core-js": "2.4.1",
    "rxjs": "^5.1.0",
